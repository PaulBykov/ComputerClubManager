<Application x:Class="ComputerClub.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ComputerClub"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
             StartupUri="View/windows/LoginWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="assets/resourses/Colors.xaml" />
                <ResourceDictionary Source="assets/fonts/Fonts.xaml"/>
                <ResourceDictionary Source="assets/styles/Scrollbar.xaml"/>
                <ResourceDictionary Source="assets/styles/Table.xaml"/>
                <ResourceDictionary Source="assets/styles/Button.xaml"/>
                <ResourceDictionary Source="assets/styles/Combobox_mini.xaml"/>
                <ResourceDictionary Source="assets/styles/Combobox_mini_light.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
    
</Application>
